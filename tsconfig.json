{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2016",
        "moduleResolution": "node",
        "pretty": true,
        "sourceMap": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "outDir": "./dist",
        "baseUrl": "src",
        // "rootDir": "./",
        "paths": {
            "@/*": [
                "*"
            ]
        },
    },
    "ts-node": {
        "require": [
            "tsconfig-paths/register"
        ]
    },
    // "include": [
    //     "src/**/*.ts"
    // ],
    "exclude": [
        "script",
        "node_modules"
    ]
}
